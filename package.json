{
  "name": "void-vscode",
  "displayName": "Void Language",
  "description": "VSCode Support For The Void Programming Language",
  "version": "0.0.6",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:mass-lang/vscode.git"
  },
  "publisher": "voidlanguage",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "void",
        "aliases": [
          "Void",
          "void",
          "Void Lang",
          "Void Language",
          "Void Programming Language",
          "void-lang",
          "voidlanguage"
        ],
        "extensions": [
          ".v",
          ".void"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "void",
        "scopeName": "index.dm",
        "path": "./syntaxes/void.tmLanguage.json"
      }
    ]
  }
}
